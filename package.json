{
  "name": "tutorialtoken",
  "version": "1.0.0",
  "description": "",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "ganache-cli",
    "start": "truffle develop",
    "build": "truffle compile --config=contracts/v5/config.js && truffle compile --config=contracts/v6/config.js",
    "test": "truffle test --show-events --config=contracts/v5/config.js && truffle test --show-events --config=contracts/v6/config.js",
    "flatten": "truffle-flattener contracts/v5/Cascade.sol | awk '/SPDX-License-Identifier/&&c++>0 {next} 1' > scratch/Cascade.sol && truffle-flattener contracts/v6/OmsX.sol | awk '/SPDX-License-Identifier/&&c++>0 {next} 1' > scratch/OmsX.sol",
    "run95": "eth95 ./build/contracts -p 3002"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "app-root-path": "2.1.0",
    "zos-lib": "^2.2.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.4.0",
    "bignumber.js": "^9.0.1",
    "chai": "^4.1.2",
    "chai-bignumber": "^2.0.2",
    "eth95": "^0.1.3",
    "openzeppelin-eth": "2.0.2",
    "stochasm": "^0.5.0",
    "truffle": "^5.4.30",
    "truffle-flattener": "^1.5.0"
  }
}
